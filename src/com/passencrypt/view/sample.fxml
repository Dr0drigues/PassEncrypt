<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
         minWidth="-Infinity"
         prefHeight="563.0" prefWidth="770.0" type="BorderPane" xmlns="http://javafx.com/javafx/8"
         fx:controller="com.passencrypt.controller.PassController">
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
    <center>
        <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="pswdLabel" layoutX="54.0" layoutY="42.0" prefHeight="17.0" prefWidth="181.0"
                       text="Password to encrypt"/>
                <TextField fx:id="pswdTxField" layoutX="171.0" layoutY="36.0" prefHeight="29.0" prefWidth="268.0"/>
                <Label fx:id="pswdHelp" layoutX="453.0" layoutY="43.0" text="?"/>
                <Label fx:id="encryptLabel" layoutX="79.0" layoutY="89.0" text="Encryption type"/>
                <Label fx:id="encryptHelp" layoutX="451.0" layoutY="89.0" text="?"/>
                <Label fx:id="resultLabel" layoutX="123.0" layoutY="211.0" prefHeight="17.0" prefWidth="40.0"
                       text="Result:"/>
                <Label fx:id="resultText" layoutX="169.0" layoutY="211.0" prefHeight="17.0" prefWidth="350.0"/>
                <ProgressIndicator fx:id="resultLoad" layoutX="719.0" layoutY="492.0" progress="0.0"/>
                <Button fx:id="generateBtn" layoutX="221.0" layoutY="178.0" mnemonicParsing="false" onAction="#generate"
                        prefHeight="16.0" prefWidth="100.0" text="Generate!"/>
                <ComboBox fx:id="encryptList" layoutX="172.0" layoutY="82.0" prefHeight="31.0" prefWidth="267.0"/>
                <Label fx:id="paramHelp" layoutX="453.0" layoutY="141.0" text="?"/>
                <TextField fx:id="paramTxField" layoutX="171.0" layoutY="134.0" prefHeight="29.0" prefWidth="268.0"/>
                <Label fx:id="paramLabel" layoutX="97.0" layoutY="141.0" prefHeight="17.0" prefWidth="181.0"
                       text="Parameters"/>
            </children>
        </Pane>
    </center>
</fx:root>
